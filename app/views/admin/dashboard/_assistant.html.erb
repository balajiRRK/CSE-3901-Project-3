<% @assistants.each do |assistant|%>
  <div class = 'assistant_container'>
    <p>
      <strong>Name:</strong>
      <%= assistant.fname %>
    </p>
    <p>
      <strong>Email:</strong>
      <%= assistant.email %>
    </p>
    <p>
      <strong>Course:</strong>
      <%= assistant.course.catalog_number+":" %>
      <%= assistant.course.title+"\n" %>
    </p>
    <p>
      <strong>Course ID:</strong>
      <%= assistant.course_id %>
    </p>
    <%if assistant.section != nil%>
      <p>
        <strong>Class number:</strong>
        <%=assistant.section.class_number %>
      </p>
      <p>
        <strong>Section:</strong>
        <%= assistant.section.section %>
      </p>
    <%end%>
  </div>
  <%= link_to "Show this assistant", assistant, class: 'btn btn-primary' %>
  <%= link_to "Look up this assistant", 'https://www.osu.edu/search/?query=' +assistant.email, class: 'btn btn-success'%>
<%end%>
<%@pagy_assistants%>
